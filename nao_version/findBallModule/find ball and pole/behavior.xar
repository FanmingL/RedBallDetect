            self.motionProxy.angleInterpolation(self.names, self.angles, self.timeLists, self.isAbsolute)
            self.onStopped(p)

        pass
    def onInput_input(self):
        self.pole_info_flag=False
    def onInput_input_pole(self, p):
        self.pole_info=p
        self.pole_info_flag=True]]>
