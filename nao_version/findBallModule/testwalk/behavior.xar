        import math
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self,p):
        self.motion.setMoveArmsEnabled(False,False)
        self.motion.moveTo(0.5, 0.3, 0.3,self.moveConfig)
        self.motion.stopMove()
        self.onStopped()]]>
